{
  "name": "AI Image Generator",
  "description": "Generate an image from a text description using DALL-E",
  "nodes": [
    {
      "id": "input-1",
      "type": "user_input",
      "name": "Image Description",
      "config": {
        "prompt": "Describe the image you want to create",
        "input_key": "description",
        "type": "text"
      },
      "position": {
        "x": 100,
        "y": 100
      }
    },
    {
      "id": "llm-1",
      "type": "llm_text_generation",
      "name": "Enhance Prompt",
      "config": {
        "provider": "openai",
        "model": "gpt-4",
        "prompt": "Transform this image description into a detailed, artistic prompt for DALL-E:\n\n{{input-1.output}}\n\nAdd details about style, lighting, composition, and mood. Keep it under 400 characters.",
        "temperature": 0.7,
        "max_tokens": 200
      },
      "position": {
        "x": 300,
        "y": 100
      }
    },
    {
      "id": "image-1",
      "type": "image_generation",
      "name": "Generate Image",
      "config": {
        "provider": "openai",
        "model": "dall-e-3",
        "prompt": "{{llm-1.text}}",
        "size": "1024x1024",
        "quality": "hd"
      },
      "position": {
        "x": 500,
        "y": 100
      }
    },
    {
      "id": "output-1",
      "type": "output",
      "name": "Image Output",
      "config": {
        "format": "json"
      },
      "position": {
        "x": 700,
        "y": 100
      }
    }
  ],
  "connections": [
    {
      "from_node": "input-1",
      "to_node": "llm-1"
    },
    {
      "from_node": "llm-1",
      "to_node": "image-1"
    },
    {
      "from_node": "image-1",
      "to_node": "output-1"
    }
  ]
}


